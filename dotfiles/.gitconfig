[filter "media"]
  clean = git-media-clean %f
  smudge = git-media-smudge %f
[core]
  editor = code --wait
  autocrlf = false
  safecrlf = true
  pager = diff-so-fancy | less --tabs=2 -RFX
[color]
  status = auto
  branch = auto
  interactive = auto
  diff = auto
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[push]
  default = simple
[user]
